{
  "expo": {
    "name": "PDA.OK",
    "slug": "pda-ok",
    "owner": "chronicmic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f23"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.michaelmistree.pdataskapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0f0f23"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.michaelmistree.pdataskapp"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "updates": {
      "enabled": false
    },
    "extra": {
      "router": {},
      "EXPO_PUBLIC_TEST_MODE": "false",
      "EXPO_PUBLIC_ANALYSIS_URL": "",
      "EXPO_PUBLIC_GEMINI_API_KEY": ""
    }
  }
}